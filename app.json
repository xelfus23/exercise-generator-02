{
    "expo": {
        "name": "AI Fitness App",
        "slug": "exercise-generator-02",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./src/assets/images/app/icon.png",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./src/assets/images/app/adaptive-icon.png",
                "backgroundColor": "#f0f3fb"
            },
            "package": "com.ai.fit.recommender",
            "splash": {
                "image": "./src/assets/images/app/splash-icon.png",
                "resizeMode": "contain",
                "backgroundColor": "#0045f5"
            }
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./src/assets/images/app/favicon.png"
        },
        "plugins": ["expo-router", "expo-splash-screen"],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {
                "origin": false
            },
            "eas": {
                "projectId": "c45f853a-7b38-4f6f-8584-bb325fb9ed3b"
            }
        },
        "owner": "suflex"
    }
}
